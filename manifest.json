{
  "name": "Ants",
  "description": "A life-changing productivity hack",
  "version": "2.0",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "action": {
      "default_title": "ReadySetGo",
      "default_icon": "./img/bugKiller.png",
      "default_popup": "hello.html"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "img/*"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "manifest_version": 3
}
