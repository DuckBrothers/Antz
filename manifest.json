{
  "name": "Ants",
  "description": "A life-changing productivity hack",
  "version": "1.3",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "action": {
      "default_title": "ReadySetGo",
      "default_icon": "./img/bugKiller.png",
      "default_popup": "hello.html"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css" : ["./src/styles.css"]
    }
],
  "manifest_version": 3
}
